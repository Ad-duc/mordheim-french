    <selectionEntry type="upgrade" import="true" name="Chain Sticks" hidden="false" id="303f-83a6-d26c-cadf">
      <costs>
        <cost name="pts" typeId="points" value="20"/>
        <cost name="Warband Rating" typeId="wb-rating" value="0"/>
      </costs>
      <profiles>
        <profile name="Chain Sticks" hidden="false" id="9f2d-1550-40c0-827e" typeId="9db87680-6ee5-b46c-48ca-dcd1c5de1bad" typeName="HtH Weapon">
          <characteristics>
            <characteristic name="Str" id="67b-aa23-f219-5ad5" hidden="false" typeId="f10cfcb7-b71e-4c27-9836-75d341e28f68">As User</characteristic>
            <characteristic name="Special" id="a9fb-d361-1c47-15d0" hidden="false" typeId="80dd3fd5-3811-af0b-e182-2ecbc7ad5d8e">Flurry, Two Handed</characteristic>
