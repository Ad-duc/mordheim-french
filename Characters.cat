          <profiles>
            <profile id="afdcb396-f0e9-8a62-7ba6-8df2d0ed2be4" name="Warhorse" page="0" hidden="false" typeId="e1beaa44-e54d-dd6b-d1f2-446b333c9bb9" typeName="Model">
