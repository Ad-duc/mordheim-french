          <rules>
            <rule id="a861-2a39-d4cb-9196" name="Stream of Corruption" hidden="false">
              <description>The Tainted One can spew forth a grotesque stream of maggots, entrails and filth.
This is counted as a shooting attack with a range of 6&quot; and is resolved at Strength 3 with no saves for armour.</description>
            </rule>
          </rules>
          <entryLinks>
            <entryLink id="e107-1a65-a2b8-86ac" name="Not First Blessing" hidden="false" collective="false" import="true" targetId="1858-1ccf-7e28-f6e5" type="selectionEntry"/>
